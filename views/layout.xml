<template id="theme_zen.theme_zen_blue" name="Theme Zen" inherit_id="website.theme" optional="disabled">
    <xpath expr="//link[@id='bootstrap_css']" position="replace">
        <link rel="stylesheet" type="text/css" href="/theme_zen/static/css/theme_zen_blue.css" t-ignore="true"/>
    </xpath>
</template>
